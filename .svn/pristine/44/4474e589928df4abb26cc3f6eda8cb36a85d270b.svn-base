<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" 
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:f="http://java.sun.com/jsf/core" 
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:p="http://primefaces.org/ui">
         

    <ui:composition template="/WEB-INF/facelets/templates/#{Login.usuarioRol}.xhtml">
	<ui:define name="title">
		Gesti√≥n Pericial - Alta de Grupo de Taller
	</ui:define>
        <ui:define name="content">
            
            <center>
    
             
 <h:form>  
 
 

  
   
     <p:panelGrid  columns="2" style="margin-top:30px; text-align: left" id="contenedor">  
 <f:facet name="header">  
 Nuevo Grupo de Taller
 </f:facet>
       
       
     
   
   
    <h:outputLabel for="denominacion" value="Denominacion : *" />
    <p:watermark for="denominacion" value="Denominacion del Grupo de Taller">
        <p:inputText id="denominacion" value="#{grupotallerManagedBean.actualNuevo.denominacion}" label="Denominacion" required="true" 
                     requiredMessage="Denominacion de Grupo de Taller Obligatorio">           
    </p:inputText>      

    </p:watermark>
  
    
    
    <h:outputLabel for="login" value="Login : " style="text-align: left" />  
    <p:watermark for="login" value="Login" >
        <p:inputText id="login" value="#{grupotallerManagedBean.actualNuevo.login}"  
                 label="Login"/>  
    </p:watermark>
   
    
   <h:outputLabel value="Baja: " for="baja"/> 
   <p:watermark for="baja" value="Baja S/N">
   <p:selectOneMenu value="#{grupotallerManagedBean.actualNuevo.baja}" required="true" id="baja" >
                      
            <f:selectItem itemLabel="N" itemValue="N" />              
            <f:selectItem itemLabel="S" itemValue="S" />  
            
   </p:selectOneMenu> 
   </p:watermark>
   
   
            <f:facet name="footer">  
                <center>
           
                    <p:commandButton action="#{grupotallerManagedBean.newRegister()}" value="Guardar" />
                    
                    
                  
                </center>
             </f:facet>  
       
         </p:panelGrid>
     
     <table>
         <tr>
             <td>
   <p:message for="denominacion"/>
  
             </td>
         </tr>
     </table>
  

</h:form>  
                
                
                
   </center>  
 
</ui:define>

</ui:composition>
    
</html>           

